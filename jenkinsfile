pipeline{
    agent any
    
    stages{
        stage("Git Checkout"){
            steps{
                git credentialsId: 'github', url: 'https://github.com/AnirudhKrishna-Veerla/hello-world/'
            }
        }
        stage("Maven Build"){
            steps{
                sh "mvn clean package"
               }
        }        
    }
}
