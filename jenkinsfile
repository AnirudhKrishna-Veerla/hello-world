pipeline{
    agent any
    environment{
        PATH = "/opt/maven/bin:$PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
                git 'https://github.com/AnirudhKrishna-Veerla/hello-world.git'
            }
        }
        stage("Maven Build"){
            steps{
                sh "mvn clean package"
              }
        }        
    }
}
